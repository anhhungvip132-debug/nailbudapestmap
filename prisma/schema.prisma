generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Booking {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  email     String?
  service   String
  note      String?
  createdAt DateTime @default(now())
}
